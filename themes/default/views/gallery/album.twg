
<div class="content-heading">
    <div class="container">
        <div class="row">
            <div class="span12">
                <h1>{{album.title}}</h1>
            </div>
        </div>
    </div>
</div>

<div class="container">
    <div>
        {% for image in images %}
            <a href="{{ image.image|thumb('940x540') }}"><img alt="{{ image.title }}" src="{{ image.image|thumb('72x48', { 'grayscale': true }) }}"></a>
        {% endfor %}
    </div>
</div>