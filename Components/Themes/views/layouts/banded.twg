<!DOCTYPE html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7"><![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8"><![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9"><![endif]-->
<!--[if gt IE 8]><html class="no-js"><!--<![endif]-->
<head>
    <meta name="keywords" content="{{ _meta.keywords }}">
    <meta name="description" content="{{ _meta.description }}">
    <meta name="viewport" content="width=device-width">

    <link rel="stylesheet" type="text/css" href="{{ themeUrl }}/assets/less/theme.less.css"/>

    <script data-main="site" src="/assets/require.js"></script>
    {% block head '' %}
</head>
<body>

<div class="row">
    <div class="col-span-3">
        {% block col1 'col1' %}
    </div>
    <div class="col-span-4 col-offset-5">
        {% block col2 'col2' %}
    </div>
</div>
<div class="row">
    <div class="col-span-12">
        {% block body 'body' %}
    </div>
</div>
<div class="row">
    <div class="col-span-4">
        <span style="height: 50px;"></span>
    </div>
    <div class="col-span-8">
        <span style="height: 50px;"></span>
    </div>
</div>
<div class="row">
    <div class="col-span-8">
        <span style="height: 50px;"></span>
    </div>
    <div class="col-span-4">
        <span style="height: 50px;"></span>
    </div>
</div>

</body>
</html>